SECRET_KEY=your-secret-key
OAUTH_REDIRECT_URL=https://your-domain.com/api/auth/callback/google
BACKEND_CORS_ORIGINS=["https://your-domain.com"]
BACKEND_PORT=8033
POSTGRES_PORT=5433 